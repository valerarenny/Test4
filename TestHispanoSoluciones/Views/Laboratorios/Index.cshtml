@model IEnumerable<TestHispanoSoluciones.Models.Laboratorio>

@{
    ViewBag.Title = "Laboratorio";
}

<h2>Laboratorio</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Docente.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PersonalAdministrativo.Nombres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status.Status1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoLab.Tipo_Lab)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CapMax)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NroEquipos)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Docente.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PersonalAdministrativo.Nombres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status.Status1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoLab.Tipo_Lab)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CapMax)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NroEquipos)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.Id_Lab }, new { @class = "btn btn-default" })
            @Html.ActionLink("Detalles", "Details", new { id = item.Id_Lab }, new { @class = "btn btn-primary" })
            @Html.ActionLink("Borrar", "Delete", new { id = item.Id_Lab }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
