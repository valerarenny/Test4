@model IEnumerable<TestHispanoSoluciones.Models.TipoLab>

@{
    ViewBag.Title = "Tipos de laboratorio";
}

<h2>Tipos de laboratorio</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tipo_Lab)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo_Lab)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.Id_TipoLab }, new { @class = "btn btn-default" })
            @Html.ActionLink("Detalles", "Details", new { id = item.Id_TipoLab }, new { @class = "btn btn-primary" })
            @Html.ActionLink("Borrar", "Delete", new { id = item.Id_TipoLab }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
